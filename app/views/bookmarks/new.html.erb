
<div class="container w-50">
  <h1>New bookmark</h1>
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.association :movie %>
    <%= f.input :comment %>
    <%= f.button :submit, class:"btn btn-primary" %>
  <% end %>
  <%= link_to "Back to list", list_path(@list), class: "btn btn-success mt-4"%>
</div>
